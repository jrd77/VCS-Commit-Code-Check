<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.atzuche.config.server.mapper.master.CityMapper">
    <resultMap id="base" type="com.atzuche.config.common.entity.CityEntity">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="code" jdbcType="INTEGER" property="code"/>
        <result column="name" jdbcType="VARCHAR" property="name"/>
        <result column="detail" jdbcType="VARCHAR" property="detail"/>
        <result column="lat" jdbcType="VARCHAR" property="lat"/>
        <result column="lon" jdbcType="VARCHAR" property="lon"/>
        <result column="telPrefix" jdbcType="VARCHAR" property="telprefix"/>
        <result column="platePrefix" jdbcType="VARCHAR" property="plateprefix"/>
        <result column="type" jdbcType="CHAR" property="type"/>
        <result column="isSupport" jdbcType="CHAR" property="issupport"/>
        <result column="is_open" jdbcType="CHAR" property="isOpen"/>
        <result column="create_date" jdbcType="TIMESTAMP" property="createDate"/>
        <result column="calculate_flag" jdbcType="INTEGER" property="calculateFlag"/>
        <result column="trusteeship_no_hour" jdbcType="INTEGER" property="trusteeshipNoHour"/>
        <result column="trusteeship_hour" jdbcType="INTEGER" property="trusteeshipHour"/>
        <result column="srv_get_cost" jdbcType="INTEGER" property="srvGetCost"/>
        <result column="srv_return_cost" jdbcType="INTEGER" property="srvReturnCost"/>
        <result column="order_time_set" jdbcType="VARCHAR" property="orderTimeSet"/>
        <result column="rent_time_set" jdbcType="VARCHAR" property="rentTimeSet"/>
        <result column="min_price" jdbcType="INTEGER" property="minPrice"/>
        <result column="float_percent" jdbcType="INTEGER" property="floatPercent"/>
        <result column="fall_percent" jdbcType="INTEGER" property="fallPercent"/>
        <result column="revert_st_interval" jdbcType="INTEGER" property="revertStInterval"/>
        <result column="revert_service_max" jdbcType="INTEGER" property="revertServiceMax"/>
        <result column="rent_st_interval" jdbcType="INTEGER" property="rentStInterval"/>
        <result column="rent_service_max" jdbcType="INTEGER" property="rentServiceMax"/>
        <result column="bg_url" jdbcType="VARCHAR" property="bgUrl"/>
        <result column="description" jdbcType="VARCHAR" property="description"/>
        <result column="support_one_key" jdbcType="TINYINT" property="supportOneKey"/>
        <result column="income_float_percent" jdbcType="INTEGER" property="incomeFloatPercent"/>
        <result column="income_fall_percent" jdbcType="INTEGER" property="incomeFallPercent"/>
        <result column="allow_transaction" jdbcType="BIT" property="allowTransaction"/>
        <result column="is_dispatching" jdbcType="INTEGER" property="isDispatching"/>
        <result column="dispatching_begin_time" jdbcType="INTEGER" property="dispatchingBeginTime"/>
        <result column="dispatching_end_time" jdbcType="INTEGER" property="dispatchingEndTime"/>
        <result column="min_subsidy_per_day" jdbcType="INTEGER" property="minSubsidyPerDay"/>
        <result column="max_subsidy_per_day" jdbcType="INTEGER" property="maxSubsidyPerDay"/>
