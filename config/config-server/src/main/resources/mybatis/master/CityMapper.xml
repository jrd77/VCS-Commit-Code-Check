        <result column="is_cpic_coupon" jdbcType="INTEGER" property="isCpicCoupon"/>
        <result column="before_time" jdbcType="INTEGER" property="beforeTime"/>
        <result column="cpic_before_time_work" jdbcType="INTEGER" property="cpicBeforeTimeWork"/>
        <result column="cpic_before_time_free" jdbcType="INTEGER" property="cpicBeforeTimeFree"/>
        <result column="address_range_version" jdbcType="INTEGER" property="addressRangeVersion"/>
        <result column="address_range_description" jdbcType="VARCHAR" property="addressRangeDescription"/>
        <result column="is_quick_pay" jdbcType="INTEGER" property="isQuickPay"/>
        <result column="full_letter" jdbcType="VARCHAR" property="fullLetter"/>
        <result column="is_hot_city" jdbcType="BIT" property="isHotCity"/>
        <result column="center_address" jdbcType="VARCHAR" property="centerAddress"/>
        <result column="is_limit" jdbcType="BIT" property="isLimit"/>
        <result column="first_letter" jdbcType="VARCHAR" property="firstLetter"/>
        <result column="is_distribute" jdbcType="INTEGER" property="isDistribute"/>
        <result column="level" jdbcType="INTEGER" property="level"/>
        <result column="peak_time_slot" jdbcType="VARCHAR" property="peakTimeSlot"/>
        <result column="before_trans_time_span" jdbcType="INTEGER" property="beforeTransTimeSpan"/>
        <result column="unservice_begin_time" jdbcType="INTEGER" property="unserviceBeginTime"/>
        <result column="unservice_end_time" jdbcType="INTEGER" property="unserviceEndTime"/>
        <result column="delivery_service_cost" jdbcType="INTEGER" property="deliveryServiceCost"/>
        <result column="package_delivery_service_cost" jdbcType="INTEGER" property="packageDeliveryServiceCost"/>
