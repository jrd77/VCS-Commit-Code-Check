<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.atzuche.config.server.mapper.master.CarParamHotBrandDepositMapper">
    <resultMap id="base" type="com.atzuche.config.common.entity.CarParamHotBrandDepositEntity">
        <result column="brand_id" property="brandId"></result>
        <result column="brand_txt" property="brandTxt"></result>
        <result column="type_id" property="typeId"></result>
        <result column="type_txt" property="typeTxt"></result>
        <result column="config_value" property="configValue"></result>
    </resultMap>


    <select id="findAll" resultMap="base">
        SELECT
        brand_id,brand_txt,type_id,type_txt,config_value
        FROM car_param_hot_brand_deposit
    </select>
</mapper>