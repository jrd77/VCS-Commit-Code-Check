<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.atzuche.config.server.mapper.master.SysConfigMapper">
    <resultMap id="base" type="com.atzuche.config.common.entity.SysConfigEntity">
         <result column="app_type" property="appType"></result>
         <result column="item_name" property="itemName"></result>
         <result column="item_key" property="itemKey"></result>
         <result column="item_value" property="itemValue"></result>
         <result column="value_type" property="valueType"></result>
    </resultMap>
    <select id="findAll" resultMap="base">
        SELECT
        app_type,
        item_name,
        item_value,
        item_key,
        value_type
        FROM sys_config
    </select>
</mapper>