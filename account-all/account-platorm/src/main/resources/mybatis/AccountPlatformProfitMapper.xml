<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.atzuche.order.accountplatorm.mapper.AccountPlatformProfitMapper">

    <resultMap type="com.atzuche.order.accountplatorm.entity.AccountPlatformProfitEntity" id="BaseResultMap">
        <result property="id" column="id"/>
        <result property="orderNo" column="order_no"/>
        <result property="ownerSubsidyAmt" column="owner_subsidy_amt"/>
        <result property="subsidyOwnerAmt" column="subsidy_owner_amt"/>
        <result property="ownerIncomeAmt" column="owner_income_amt"/>
        <result property="renterCost" column="renter_cost"/>
        <result property="renterSubsidyAmt" column="renter_subsidy_amt"/>
        <result property="subsidyRenterAmt" column="subsidy_renter_amt"/>
        <result property="platformSubsidyAmt" column="platform_subsidy_amt"/>
        <result property="platformReceivableAmt" column="platform_receivable__amt"/>
        <result property="platformReceivedAmt" column="platform_received__amt"/>
        <result property="version" column="version"/>
        <result property="status" column="status"/>
        <result property="createTime" column="create_time"/>
        <result property="createOp" column="create_op"/>
        <result property="updateTime" column="update_time"/>
        <result property="updateOp" column="update_op"/>
        <result property="isDelete" column="is_delete"/>
    </resultMap>


	<sql id="Base_Column_List">
	`id`, 	
	`order_no`, 	
	`owner_subsidy_amt`, 	
	`subsidy_owner_amt`, 	
	`owner_income_amt`, 	
	`renter_cost`, 	
	`renter_subsidy_amt`, 	
	`subsidy_renter_amt`, 	
	`platform_subsidy_amt`, 	
	`platform_receivable__amt`, 	
	`platform_received__amt`, 	
	`version`, 	
	`status`, 	
	`create_time`, 	
	`create_op`, 	
	`update_time`, 	
	`update_op`, 	
	`is_delete`	

	</sql>


	<select id="selectByPrimaryKey" resultMap="BaseResultMap">
		select <include refid="Base_Column_List" /> from account_platform_profit where id = #{value}
	</select>

	<insert id="insert" parameterType="com.atzuche.order.accountplatorm.entity.AccountPlatformProfitEntity" useGeneratedKeys="true" keyProperty="id">
		insert into account_platform_profit
		(
				`order_no`,		
				`owner_subsidy_amt`,		
				`subsidy_owner_amt`,		
				`owner_income_amt`,		
				`renter_cost`,		
				`renter_subsidy_amt`,		
				`subsidy_renter_amt`,		
				`platform_subsidy_amt`,		
				`platform_receivable__amt`,		
				`platform_received__amt`,		
				`version`,		
				`status`,		
				`create_time`,		
				`create_op`,		
				`update_time`,		
				`update_op`,		
				`is_delete`		
			)
		values
		(
				#{orderNo}, 		
				#{ownerSubsidyAmt}, 		
				#{subsidyOwnerAmt}, 		
				#{ownerIncomeAmt}, 		
				#{renterCost}, 		
				#{renterSubsidyAmt}, 		
				#{subsidyRenterAmt}, 		
				#{platformSubsidyAmt}, 		
				#{platformReceivableAmt}, 		
				#{platformReceivedAmt}, 		
				#{version}, 		
				#{status}, 		
				#{createTime}, 		
				#{createOp}, 		
				#{updateTime}, 		
				#{updateOp}, 		
				#{isDelete}		
			)
	</insert>

	<update id="updateByPrimaryKeySelective" parameterType="com.atzuche.order.accountplatorm.entity.AccountPlatformProfitEntity">
		update account_platform_profit 
		<set>
				<if test="orderNo != null">`order_no` = #{orderNo}, </if>
				<if test="ownerSubsidyAmt != null">`owner_subsidy_amt` = #{ownerSubsidyAmt}, </if>
				<if test="subsidyOwnerAmt != null">`subsidy_owner_amt` = #{subsidyOwnerAmt}, </if>
				<if test="ownerIncomeAmt != null">`owner_income_amt` = #{ownerIncomeAmt}, </if>
				<if test="renterCost != null">`renter_cost` = #{renterCost}, </if>
				<if test="renterSubsidyAmt != null">`renter_subsidy_amt` = #{renterSubsidyAmt}, </if>
				<if test="subsidyRenterAmt != null">`subsidy_renter_amt` = #{subsidyRenterAmt}, </if>
				<if test="platformSubsidyAmt != null">`platform_subsidy_amt` = #{platformSubsidyAmt}, </if>
				<if test="platformReceivableAmt != null">`platform_receivable__amt` = #{platformReceivableAmt}, </if>
				<if test="platformReceivedAmt != null">`platform_received__amt` = #{platformReceivedAmt}, </if>
				version = version + 1,
				<if test="status != null">`status` = #{status}, </if>
				<if test="createTime != null">`create_time` = #{createTime}, </if>
				<if test="createOp != null">`create_op` = #{createOp}, </if>
				<if test="updateTime != null">`update_time` = #{updateTime}, </if>
				<if test="updateOp != null">`update_op` = #{updateOp}, </if>
				<if test="isDelete != null">`is_delete` = #{isDelete}</if>
			</set>
		where id = #{id} and version = #{version}
	</update>

    <select id="getAccountPlatformProfit" resultMap="BaseResultMap">
        select <include refid="Base_Column_List" /> from account_platform_profit where order_no = #{orderNo} limit 1
    </select>

</mapper>