<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.atzuche.order.accountrenterwzdepost.mapper.AccountRenterWzDepositMapper">

    <resultMap type="com.atzuche.order.accountrenterwzdepost.entity.AccountRenterWzDepositEntity" id="BaseResultMap">
        <result property="id" column="id"/>
        <result property="orderNo" column="order_no"/>
        <result property="memNo" column="mem_no"/>
        <result property="yingshouDeposit" column="yingshou_deposit"/>
        <result property="shishouDeposit" column="shishou_deposit"/>
        <result property="isAuthorize" column="is_authorize"/>
        <result property="isFreeDeposit" column="is_free_deposit"/>
        <result property="shouldReturnDeposit" column="should_return_deposit"/>
        <result property="realReturnDeposit" column="real_return_deposit"/>
        <result property="settleStatus" column="settle_status"/>
        <result property="settleTime" column="settle_time"/>
        <result property="version" column="version"/>
        <result property="createTime" column="create_time"/>
        <result property="createOp" column="create_op"/>
        <result property="updateTime" column="update_time"/>
        <result property="updateOp" column="update_op"/>
        <result property="isDelete" column="is_delete"/>
    </resultMap>


	<sql id="Base_Column_List">
	`id`, 	
	`order_no`, 	
	`mem_no`, 	
	`yingshou_deposit`, 	
	`shishou_deposit`, 	
	`is_authorize`, 	
	`is_free_deposit`, 	
	`should_return_deposit`, 	
	`real_return_deposit`, 	
	`settle_status`, 	
	`settle_time`, 	
	`version`, 	
	`create_time`, 	
	`create_op`, 	
	`update_time`, 	
	`update_op`, 	
	`is_delete`	

	</sql>


	<select id="selectByPrimaryKey" resultMap="BaseResultMap">
		select <include refid="Base_Column_List" /> from account_renter_wz_deposit where id = #{value}
	</select>

	 
	<insert id="insert" parameterType="com.atzuche.order.accountrenterwzdepost.entity.AccountRenterWzDepositEntity" useGeneratedKeys="true" keyProperty="id">
		insert into account_renter_wz_deposit
		(
				`order_no`,		
				`mem_no`,		
				`yingshou_deposit`,		
				`shishou_deposit`,		
				`is_authorize`,		
				`is_free_deposit`,		
				`should_return_deposit`,		
				`real_return_deposit`,		
				`settle_status`,		
				`settle_time`,		
				`version`,		
				`create_time`,		
				`create_op`,		
				`update_time`,		
				`update_op`,		
				`is_delete`		
			)
		values
		(
				#{orderNo}, 		
				#{memNo}, 		
				#{yingshouDeposit}, 		
				#{shishouDeposit}, 		
				#{isAuthorize}, 		
				#{isFreeDeposit}, 		
				#{shouldReturnDeposit}, 		
				#{realReturnDeposit}, 		
				#{settleStatus}, 		
				#{settleTime}, 		
				#{version}, 		
				#{createTime}, 		
				#{createOp}, 		
				#{updateTime}, 		
				#{updateOp}, 		
				#{isDelete}		
			)
	</insert>
	
	<update id="updateByPrimaryKeySelective" parameterType="com.atzuche.order.accountrenterwzdepost.entity.AccountRenterWzDepositEntity">
		update account_renter_wz_deposit
		<set>
				<if test="orderNo != null">`order_no` = #{orderNo}, </if>
				<if test="memNo != null">`mem_no` = #{memNo}, </if>
				<if test="yingshouDeposit != null">`yingshou_deposit` = #{yingshouDeposit}, </if>
				<if test="shishouDeposit != null">`shishou_deposit` = #{shishouDeposit}, </if>
				<if test="isAuthorize != null">`is_authorize` = #{isAuthorize}, </if>
				<if test="isFreeDeposit != null">`is_free_deposit` = #{isFreeDeposit}, </if>
				<if test="shouldReturnDeposit != null">`should_return_deposit` = #{shouldReturnDeposit}, </if>
				<if test="realReturnDeposit != null">`real_return_deposit` = #{realReturnDeposit}, </if>
				<if test="settleStatus != null">`settle_status` = #{settleStatus}, </if>
				<if test="settleTime != null">`settle_time` = #{settleTime}, </if>
				version = version +1 ,
				<if test="createTime != null">`create_time` = #{createTime}, </if>
				<if test="createOp != null">`create_op` = #{createOp}, </if>
				<if test="updateTime != null">`update_time` = #{updateTime}, </if>
				<if test="updateOp != null">`update_op` = #{updateOp}, </if>
				<if test="isDelete != null">`is_delete` = #{isDelete}</if>
			</set>
		where id = #{id} and version = #{version}
	</update>
    <!-- 根据订单号查询车俩租金信息 -->
    <select id="selectByOrderAndMemNo" resultMap="BaseResultMap">
        select <include refid="Base_Column_List" /> from account_renter_wz_deposit where order_no = #{orderNo} and mem_no = #{memNo} order by id desc limit 1
    </select>

</mapper>